  <servlet>
  	<servlet-name>dayMenu</servlet-name>
  	<servlet-class>member.mentee.dayMenu.controller.DayMenuServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>dayMenu</servlet-name>
  	<url-pattern>/dayMenu.do</url-pattern>
  </servlet-mapping>
  
  
  
  <!-- 운동 계획 서블릿 -->
  <servlet>
  	<servlet-name>plan</servlet-name>
  	<servlet-class>member.mentee.exercise.plan.controller.PlanServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>plan</servlet-name>
  	<url-pattern>/plan.do</url-pattern>
  </servlet-mapping>
  
  
  <!-- 총 운동량 서블릿 -->
  <servlet>
  	<servlet-name>total</servlet-name>
  	<servlet-class>member.mentee.exercise.total.controller.TotalServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>total</servlet-name>
  	<url-pattern>/total.do</url-pattern>
  </servlet-mapping>
  
  
  <!-- 음식 API 서블릿 -->
  <servlet>
  	<servlet-name>food</servlet-name>
  	<servlet-class>member.mentee.food.controller.FoodServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>food</servlet-name>
  	<url-pattern>/food.do</url-pattern>
  </servlet-mapping>
  
  
  
  <!-- 프로필 서블릿 -->
  <servlet>
  	<servlet-name>profile</servlet-name>
  	<servlet-class>member.profile.controller.ProfileServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>profile</servlet-name>
  	<url-pattern>/profile.do</url-pattern>
  </servlet-mapping>
  
  
  
  <!-- 결제 서블릿 -->
  <servlet>
  	<servlet-name>trade</servlet-name>
